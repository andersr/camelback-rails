<h1>My Assignments</h1>
<p><%= "Loaded at #{@time}" %></p>
<table>
  <thead>
    <tr>
      <th>Assignments</th><th>Forks</th>
    </tr>
  </thead>
  <tbody>
   <% @private_assignments.each do |fork| %>
    <tr>
      <td><%= fork.rels[:forks].get.data %></td>
    </tr>
  <% end %>
  </tbody>
</table>
